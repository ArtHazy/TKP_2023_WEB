generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id          String @id @map("_id")
  email       String @unique
  name        String
  password    String
  Quizzes     Quiz[]
  currentQuiz Quiz?
}

type Quiz {
  title     String
  Questions Question[]
  userId    String?
}

type Question {
  text      String
  isCorrect Boolean
}
