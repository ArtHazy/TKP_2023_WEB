generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String  @id @map("_id") 
  email    String  @unique
  name     String?
  password String
  rooms    Room[]
}

model Room {
  id String @id @map("_id") 
  User   User?   @relation(fields: [userId], references: [id])
  userId String? 
}
